read -p "Sisesta ridade arv: " rida
read -p "Sisesta tÃ¤rnide arv: " tarn
for((i = 1; i <= $rida; i++ ))
do
	echo "$i"
if [ $i -eq 1 -o $i -eq $rida ]; then

for((j = 1;j <= $tarn; j++ ))
